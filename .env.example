# Database configuration
DATABASE_URL="postgresql://<USER>:<PASSWORD>@<HOST>:<PORT>/<DATABASE>?schema=<SCHEMA>"

# Authentication configuration
JWT_SECRET="<YOUR_JWT_SECRET>"
TOKEN_MASTER="<YOUR_MASTER_TOKEN>"

# Payment API (Stripe)
STRIPE_SECRET_KEY="<YOUR_STRIPE_SECRET_KEY>"

# External APIs configuration
NEXT_PUBLIC_INFURA_URL="<YOUR_INFURA_URL>"
NEXT_PUBLIC_ALCHEMY_URL="<YOUR_ALCHEMY_URL>"

# Supabase configuration
SUPABASE_URL="<YOUR_SUPABASE_URL>"
SUPABASE_KEY="<YOUR_SUPABASE_KEY>"

# URL configuration
URL_BASE="<YOUR_BACKEND_URL>"
FRONTEND_URL="<YOUR_FRONTEND_URL>"

# Environment and port configuration
PORT="3000"
NODE_ENV="production" # Change to "development" for development

# Rate limit configuration
RATE_LIMIT_MAX="100" # Increase for development if necessary

# CORS configuration
CORS_ALLOWED_ORIGINS="<YOUR_ALLOWED_ORIGINS>"

# Logging level
LOG_LEVEL="combined" # Change to "dev" for development
